<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.newdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="RJ veg"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Material"
        tools:replace="android:label">
        <activity
            android:name=".activity.VegetableActivity"
            android:label="Vegetables"></activity>
        <activity
            android:name=".activity.FruitsActivity"
            android:label="Fruits" />
        <activity
            android:name=".activity.SplashActivity"
            android:theme="@style/AppTheme.Material.NoActionBar.Blue">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ForgotPassword" />
        <activity android:name=".activity.HomeActivity"
            android:label="RJ Veg"/>
        <activity android:name=".activity.LoginScreen" />
        <activity android:name=".activity.MainActivity" />
    </application>

</manifest>